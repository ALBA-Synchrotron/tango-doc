            <para>
                <itemizedlist>
                    <listitem>
                            <para>#define <link linkend="dc/df8/dbapi_8h_1a1001977b4dfc9f70af5f20d97ed8743d">MANAGE_EXCEPT</link> catch (Tango::CommunicationFailed &amp;e) \
	{ \
		if (e.errors.length() &gt;= 2) \
		{ \
			if (::strcmp(e.errors[1].reason.in(),&quot;API_DeviceTimedOut&quot;) == 0) \
			{ \
				if (db_retries != 0) \
				{ \
					db_retries--; \
					if (db_retries == 0) \
						throw; \
				} \
				else \
					throw; \
			} \
			else \
				throw; \
		} \
		else \
			throw; \
	}</para>
                    </listitem>
                </itemizedlist>
            </para>
