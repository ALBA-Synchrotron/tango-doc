                            <para>#define <link linkend="dc/df8/dbapi_8h_1a4982ee044887994e11e6b73dce645581">CALL_DB_SERVER</link> { \
		bool retry_mac = true; \
		long db_retries = 0; \
		if (db_tg != NULL) \
		{ \
			if (db_tg-&gt;is_svr_starting() == true) \
				db_retries = DB_START_PHASE_RETRIES; \
		} \
		while (retry_mac == true) \
		{ \
			try \
			{ \
				RET = command_inout(NAME,SEND); \
				retry_mac = false; \
			} \
			<link linkend="dc/df8/dbapi_8h_1a1001977b4dfc9f70af5f20d97ed8743d">MANAGE_EXCEPT</link>(NAME) \
		} \
	}</para>
                    </listitem>
