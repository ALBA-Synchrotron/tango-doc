                            <para>#define <link linkend="dc/df8/dbapi_8h_1a85ea3965d82f35e14eba1d170a127a0f">CALL_DB_SERVER_NO_RET</link> { \
		bool retry_mac = true; \
		long db_retries = 0; \
		if (db_tg != NULL) \
		{ \
			if (db_tg-&gt;is_svr_starting() == true) \
				db_retries = DB_START_PHASE_RETRIES; \
		} \
		while (retry_mac == true) \
		{ \
			try \
			{ \
				command_inout(NAME,SEND); \
				retry_mac = false; \
			} \
